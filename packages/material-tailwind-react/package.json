{
  "name": "material-tailwind-react",
  "homepage": "https://material-tailwind.com/react",
  "version": "1.0.0",
  "description": "Material Tailwind is an easy-to-use components library for Tailwind CSS inspired by Material Design.",
  "repository": "https://github.com/creativetimofficial/material-tailwind",
  "license": "MIT",
  "files": [
    "src"
  ],
  "scripts": {
    "build": "swc ./src -d ../@material-tailwind/react --config-file .swcrc && cp ./package.json ../@material-tailwind/react",
    "lint:check": "eslint . --ext js,jsx,ts,tsx --max-warnings=0 --config .eslintrc.json --no-eslintrc",
    "prettier:check": "prettier -c ."
  },
  "dependencies": {
    "@floating-ui/react-dom-interactions": "0.1.1",
    "classnames": "2.3.1",
    "deepmerge": "4.2.2",
    "downshift": "6.1.7",
    "framer-motion": "6.2.8",
    "material-ripple-effects": "2.0.1",
    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-merge-refs": "1.1.0"
  },
  "devDependencies": {
    "@swc/cli": "0.1.57",
    "@swc/core": "1.2.170",
    "eslint": "8.11.0",
    "eslint-config-prettier": "8.4.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.3.0",
    "prettier": "2.5.1"
  }
}
